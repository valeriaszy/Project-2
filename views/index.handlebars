<div class="container-fluid">
  <div class="row">
    <div class="col-8 offset-2">
      <h1>Recipes</h1>
    </div>
  </div>
  <div class = "col-md-11 mx-auto">
    {{#each recipeRow}}
      <div class="row justify-content-center my-3">
      {{#each this.recipe}}
        {{> recipeCard}}
      {{/each}}
      </div>
    {{/each}}
  </div>
</div>

